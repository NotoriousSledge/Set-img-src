    "@types/eslint": "^8.4.1",
    "@types/jasmine": "^4.0.0",
    "@types/karma": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "^4.0.1",
    "karma": "^6.3.17",
